{
  "version": 5,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 24,
    "patch": 0
  },
  "include": [
    "CMakePresetsBase.json"
  ],
  "configurePresets": [
    {
      "name": "mingw-gcc-release-static",
      "inherits": [
        "toolset-mingw",
        "gcc-release-static"
      ],
      "cacheVariables": {
        "CMAKE_EXE_LINKER_FLAGS": "-fuse-ld=lld",
        "CMAKE_SHARED_LINKER_FLAGS": "-fuse-ld=lld"
      }
    },
    {
      "name": "mingw-gcc-debug-static",
      "inherits": [
        "toolset-mingw",
        "gcc-debug-static"
      ],
      "cacheVariables": {
        "CMAKE_EXE_LINKER_FLAGS": "-fuse-ld=lld",
        "CMAKE_SHARED_LINKER_FLAGS": "-fuse-ld=lld"
      }
    },
    {
      "name": "mingw-clang-release-static",
      "inherits": [
        "toolset-mingw",
        "clang-release-static"
      ]
    },
    {
      "name": "mingw-clang-debug-static",
      "inherits": [
        "toolset-mingw",
        "clang-debug-static"
      ]
    },
    {
      "name": "mingw-gcc-release-shared",
      "inherits": [
        "toolset-mingw",
        "gcc-release-shared"
      ],
      "cacheVariables": {
        "CMAKE_EXE_LINKER_FLAGS": "-fuse-ld=lld",
        "CMAKE_SHARED_LINKER_FLAGS": "-fuse-ld=lld"
      }
    },
    {
      "name": "mingw-gcc-debug-shared",
      "inherits": [
        "toolset-mingw",
        "gcc-debug-shared"
      ],
      "cacheVariables": {
        "CMAKE_EXE_LINKER_FLAGS": "-fuse-ld=lld",
        "CMAKE_SHARED_LINKER_FLAGS": "-fuse-ld=lld"
      }
    },
    {
      "name": "mingw-clang-release-shared",
      "inherits": [
        "toolset-mingw",
        "clang-release-shared"
      ]
    },
    {
      "name": "mingw-clang-debug-shared",
      "inherits": [
        "toolset-mingw",
        "clang-debug-shared"
      ]
    }
  ]
}