{
  "version": 5,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 24,
    "patch": 0
  },
  "include": [
    "CMakePresetsBase.json"
  ],
  "configurePresets": [
    {
      "name": "windows-gcc-release",
      "inherits": [
        "toolset-mingw",
        "gcc-release"
      ]
    },
    {
      "name": "windows-gcc-debug",
      "inherits": [
        "toolset-mingw",
        "gcc-debug"
      ],
      "cacheVariables": {
        "OWL_ENABLE_COVERAGE": "ON"
      },
      "vendor": {
        "silmaen": {
          "release_preset": "windows-gcc-release"
        }
      }
    },
    {
      "name": "windows-clang-release",
      "inherits": [
        "toolset-mingw",
        "clang-release"
      ]
    },
    {
      "name": "windows-clang-debug",
      "inherits": [
        "toolset-mingw",
        "clang-debug"
      ],
      "cacheVariables": {
        "OWL_ENABLE_COVERAGE": "ON"
      },
      "vendor": {
        "silmaen": {
          "release_preset": "windows-clang-release"
        }
      }
    }
  ]
}
