{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 22,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "base",
      "hidden": true,
      "architecture": {
        "value": "x64",
        "strategy": "external"
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      },
      "generator": "Ninja",
      "binaryDir": "cmake-build-${presetName}"
    },
    {
      "name": "mingw-gcc-release",
      "inherits": "base",
      "vendor": {
        "jetbrains.com/clion": {
          "toolchain": "MinGW"
        }
      },
      "cacheVariables": {
        "CMAKE_CXX_COMPILER": "g++"
      }
    },
    {
      "name": "mingw-gcc-debug",
      "inherits": "mingw-gcc-release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "VW_COVERAGE": "ON"
      }
    },
    {
      "name": "mingw-clang-release",
      "inherits": "mingw-gcc-release",
      "cacheVariables": {
        "CMAKE_CXX_COMPILER": "clang++"
      }
    },
    {
      "name": "mingw-clang-debug",
      "inherits": "mingw-clang-release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "wsl-gcc-release",
      "inherits": "base",
      "vendor": {
        "jetbrains.com/clion": {
          "toolchain": "WSL"
        }
      },
      "cacheVariables": {
        "CMAKE_CXX_COMPILER": "g++"
      }
    },
    {
      "name": "wsl-gcc-debug",
      "inherits": "wsl-gcc-release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "VW_COVERAGE": "ON"
      }
    },
    {
      "name": "wsl-clang-release",
      "inherits": "wsl-gcc-release",
      "cacheVariables": {
        "CMAKE_CXX_COMPILER": "clang++"
      }
    },
    {
      "name": "wsl-clang-debug",
      "inherits": "wsl-clang-release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    }
  ]
}